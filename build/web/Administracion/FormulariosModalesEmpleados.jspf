                                    <textarea style=" resize: none;" rows="2" id="modVerDireccion" class="form-control"  maxlength="200" required="" name="txtRegDireccion" onkeypress="return soloLetrasNumerosCaracEspe(event)" placeholder="Dirección"></textarea>
                                    <%--pattern="[A-Za-z0-9Ã±]+"--%>
                                </div>
                            </div>
                            <br>

                             <center id="editTituloTutorias">
                            <h4><b>TUTORÍAS</b></h4><br>
                             </center>
                            <div class="row form" style="margin-left: 4%" id="EditTutoria">                                                               
                                <div class="col-sm-6">
